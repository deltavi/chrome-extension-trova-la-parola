{
  "name": "Trova La Parola",
  "description": "Estensione Chrome per trovare una parola italiana",
  "version": "0.1",
  "permissions": [
    "activeTab", 
    "clipboardWrite"
  ],
  "icons": {
    "16": "ico/btn_circle_text_transformer.16x16.png",
    "20": "ico/btn_circle_text_transformer.20x20.png",
    "24": "ico/btn_circle_text_transformer.24x24.png",
    "32": "ico/btn_circle_text_transformer.32x32.png",
    "48": "ico/btn_circle_text_transformer.48x48.png",
    "64": "ico/btn_circle_text_transformer.64x64.png",
    "128": "ico/btn_circle_text_transformer.128x128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  
  "action": {
    "default_title": "Trova La Parola",
    "default_popup": "word-finder.html",
    "default_icon": {
      "16": "ico/btn_circle_text_transformer.16x16.png",
      "20": "ico/btn_circle_text_transformer.20x20.png",
      "24": "ico/btn_circle_text_transformer.24x24.png",
      "32": "ico/btn_circle_text_transformer.32x32.png",
      "48": "ico/btn_circle_text_transformer.48x48.png",
      "64": "ico/btn_circle_text_transformer.64x64.png",
      "128": "ico/btn_circle_text_transformer.128x128.png"
    }
  },
  "manifest_version": 3
}





